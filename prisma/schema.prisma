// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Member {
  id Int @id @default(autoincrement())
  name String 
  birth_date DateTime
  batism_date DateTime
  titleChurch String
  cpf BigInt
  rg Int
  login String
  email String
  password String
  foto String?
}

